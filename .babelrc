{
  "presets": [
    "@babel/preset-env",
    "@babel/react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "add-module-exports",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-flow-strip-types"
  ],
  "env": {
    "development": {
      "plugins": [
        "flow-react-proptypes"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}
